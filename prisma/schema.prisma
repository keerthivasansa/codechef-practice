generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Problem {
  code       String   @id @unique
  name       String
  difficulty Int
  upvotes    Int
  tags       String[]
  score      Float
  contest    Int
}

model GlobalFilter {
  settingId Int @id

  startRating Int
  endRating   Int
  filterTags  String[]
}
